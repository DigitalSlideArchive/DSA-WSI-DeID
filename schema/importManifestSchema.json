{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "imageFilename": "meh.svs",
            "TokenID": "1234AB567001",
            "Proc_Seq": "01",
            "Spec_Site": "C44.7",
            "Slide_ID": "02",
            "ImageID": "1234AB567001_01_01 Proc_Seq_ Slide_ID",
            "PostDSAStatus": "Quarantined"
        }
    ],
    "required": [
        "InputFileName",
        "TokenID",
        "Proc_Seq",
        "Proc_Type",
        "Spec_Site",
        "Slide_ID",
        "ImageID"
        
    ],
    "properties": {
        "InputFileName": {
            "$id": "#/properties/InputFileName",
            "type": "string",
            "title": "InputFileName",
            "description": "Name of file and file type as received from scanning facility.",
            "default": "",
            "examples": [
                "129873425.svs or 12321312.ndpi"
            ]
        },
        "TokenID": {
            "$id": "#/properties/TokenID",
            "type": "string",
            "title": "The TokenID schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "1234AB567001"
            ]
        },
        "Proc_Seq": {
            "$id": "#/properties/Proc_Seq",
            "type": "string",
            "title": "The Proc_Seq schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "01"
            ]
        },
        "Proc_Type": {
            "$id": "#/properties/Proc_Type",
            "type": "string",
            "title": "The Proc_Type schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "01"
            ]
        },
        "Spec_Site": {
            "$id": "#/properties/Spec_Site",
            "type": "string",
            "title": "The Spec_Site schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "C44.7"
            ]
        },
        "Slide_ID": {
            "$id": "#/properties/Slide_ID",
            "type": "string",
            "title": "The Slide_ID schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "02"
            ]
        },
        "ImageID": {
            "$id": "#/properties/ImageID",
            "type": "string",
            "title": "The ImageID schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "1234AB567001_01_01 Proc_Seq_ Slide_ID"
            ]
        }
    },
    "additionalProperties": false
}

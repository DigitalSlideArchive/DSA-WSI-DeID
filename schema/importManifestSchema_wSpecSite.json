  "default": {},
  "examples": [
    {
      "TokenID": "1234AB567001",
      "Proc_Seq": "01",
      "Proc_Type": "Biopsy",
      "Spec_Site": "C44.7",
      "Slide_ID": "02",
      "ImageID": "1234AB567001_01_01",
      "InputFileName": "08-A.svs"
    }

      "pattern": "[0-9]{2}.{8}[0-9]{2}",
      "title": "TokenID",
      "description": "This ID should be used when communicating with IMS, in the Subject Manifest, DeID Upload File, and DeID Export File, and in the file names for the pathology reports and the WSIs.",
      "default": "",
      "examples": [
        "1234AB567001"
      ]
    },
